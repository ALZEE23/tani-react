{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/blog",
      "dest": "/"
    },
    {
      "src": "/login",
      "dest": "/"
    },
    {
      "src": "/formblog",
      "dest": "/"
    },
    {
      "src": "/formblog/:id",
      "dest": "/"
    },
    {
      "src": "/bookmark",
      "dest": "/"
    },
    {
      "src":"/register",
      "dest": "/"
    },
    {
      "src":"/blog/:id",
      "dest": "/"
    },
    {
      "src":"/myblog",
      "dest": "/"
    },
    {
      "src":"/edit-blog/:id",
      "dest": "/"
    }
  ]
}